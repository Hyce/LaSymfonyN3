
{% extends 'DevschoolFilmBundle::layout.html.twig' %}

{% block title %}Liste des Films{% endblock %}

{% block body %}

<ul class="liste">
    {% for film in films %}
        <li>
            <h2><a href="{{ path('page_livre',  {'id': film.id}) }}">{{ film.titre }}</a></h2>
        </li>
    {% endfor %}
</ul>

{% endblock %}
